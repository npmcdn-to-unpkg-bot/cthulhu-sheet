{
  "name": "cthulhu",
  "version": "0.1.0",
  "description": "Reactive character sheet for Call of Cthulu Pen And Paper RGP, 7th edition",
  "main": "app.jsx",
  "scripts": {
    "test": "mocha tests/pureFunctions/*.tests.js",
    "start": "npm run serve | npm run dev",
    "serve": "./node_modules/.bin/http-server -p 8080",
    "dev": "webpack-dev-server --progress --colors --hot --inline --port 8090",
    "serve:hot": "webpack-dev-server --hot --inline --progress --colors --watch --display-error-details --display-cached --content-base ./"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/olemak/cthulhu-sheet.git"
  },
  "keywords": [
    "react",
    "cthulhu",
    "CoC",
    "webpack",
    "roleplaying",
    "RPG"
  ],
  "author": "Ole Martin Kristiansen",
  "license": "GPL-2.0",
  "bugs": {
    "url": "https://github.com/olemak/cthulhu-sheet/issues"
  },
  "homepage": "https://github.com/olemak/cthulhu-sheet#readme",
  "dependencies": {
    "babel-preset-airbnb": "^2.0.0",
    "babel-preset-es2015": "^6.13.2",
    "babel-preset-stage-0": "^6.5.0",
    "react": "^15.2.0",
    "react-redux": "^4.4.5",
    "redux": "^3.5.2"
  },
  "devDependencies": {
    "chai": "^3.5.0",
    "css-loader": "^0.23.1",
    "enzyme": "^2.4.1",
    "http-server": "~0.7.1",
    "jsx-loader": "^0.13.2",
    "mocha": "^3.0.1",
    "node-sass": "^3.8.0",
    "react-addons-test-utils": "^15.3.0",
    "sass-loader": "^4.0.0",
    "style-loader": "^0.13.1",
    "webpack": "^1.13.1",
    "webpack-dev-server": "^1.14.1"
  }
}
